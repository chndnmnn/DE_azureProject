													"type": "Expression"
												},
												"layer": {
													"value": "@pipeline().parameters.layer",
													"type": "Expression"
												},
												"jsonPath": {
													"value": "@concat(pipeline().parameters.source,'/',pipeline().parameters.db_table,'/',pipeline().parameters.db_table,'CDC')",
													"type": "Expression"
												},
												"jsonFile": "empty.json"
											}
										}
									],
									"outputs": [
										{
											"referenceName": "ds_json_dynamic",
											"type": "DatasetReference",
											"parameters": {
												"container": {
													"value": "@pipeline().parameters.container",
													"type": "Expression"
												},
												"layer": {
													"value": "@pipeline().parameters.layer",
													"type": "Expression"
												},
												"jsonPath": {
													"value": "@concat(pipeline().parameters.source,'/',pipeline().parameters.db_table,'/',pipeline().parameters.db_table,'CDC')",
													"type": "Expression"
												},
												"jsonFile": "cdc.json"
											}
										}
									]
								}
							]
						}
					},
					{
						"name": "Is_cdcOrBackDateLoad",
						"type": "IfCondition",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@empty(pipeline().parameters.fromDate)",
								"type": "Expression"
							},
							"ifFalseActivities": [
								{
									"name": "SetFromDateFalse",
									"type": "SetVariable",
									"dependsOn": [],
									"policy": {
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"variableName": "fromDate",
										"value": {
											"value": "@pipeline().parameters.fromDate",
											"type": "Expression"
										}
									}
								}
							],
							"ifTrueActivities": [
								{
									"name": "GetCDC",
									"type": "Lookup",
									"dependsOn": [],
									"policy": {
										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"source": {
											"type": "JsonSource",
											"storeSettings": {
												"type": "AzureBlobFSReadSettings",
												"recursive": true,
												"enablePartitionDiscovery": false
											},
											"formatSettings": {
												"type": "JsonReadSettings"
											}
										},
										"dataset": {
											"referenceName": "ds_json_dynamic",
											"type": "DatasetReference",
											"parameters": {
												"container": {
													"value": "@pipeline().parameters.container",
													"type": "Expression"
												},
												"layer": {
													"value": "@pipeline().parameters.layer",
													"type": "Expression"
												},
												"jsonPath": {
													"value": "@concat(pipeline().parameters.source,'/',pipeline().parameters.db_table,'/',pipeline().parameters.db_table,'CDC')",
													"type": "Expression"
												},
												"jsonFile": "cdc.json"
											}
										},
										"firstRowOnly": false
									}
								},
								{
									"name": "SetFromDateTrue",
									"type": "SetVariable",
									"dependsOn": [
										{
											"activity": "GetCDC",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],
									"policy": {
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"variableName": "fromDate",
										"value": {
											"value": "@activity('GetCDC').output.value[0].last_load_date",
											"type": "Expression"
										}
									}
								}
							]
						}
					},
					{
						"name": "setCurrentDate",
						"type": "SetVariable",
						"dependsOn": [],
						"policy": {
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"variableName": "currentDate",
							"value": {
								"value": "@formatDateTime(utcNow(), 'yyyyMMddHHmmss')",
