												"container": {
													"value": "@pipeline().parameters.container",
													"type": "Expression"
												},
												"layer": {
													"value": "@pipeline().parameters.layer",
													"type": "Expression"
												},
												"parqPath": {
													"value": "@concat(pipeline().parameters.source,'/',pipeline().parameters.db_table,'/','Archive')",
													"type": "Expression"
												},
												"parqFile": {
													"value": "@concat(pipeline().parameters.db_table,'_',variables('currentDate'))",
													"type": "Expression"
												}
											}
										}
									]
								},
								{
									"name": "DeleteZeroDataFile",
									"type": "Delete",
									"dependsOn": [
										{
											"activity": "CopyToArchive",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],
									"policy": {
										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"dataset": {
											"referenceName": "ds_parquet_dynamic",
											"type": "DatasetReference",
