{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf0azureProject"
		},
		"ls_demo_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_demo'"
		},
		"ls_AzSqlDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dbserver0azureproject.database.windows.net"
		},
		"ls_AzSqlDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db0azureProject"
		},
		"ls_AzSqlDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azureproject"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sa0azureproject.dfs.core.windows.net/"
		},
		"ls_akv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akv-azureProject.vault.azure.net/"
		},
		"ls_demo_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsadf2learn.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/incremental_ingestion')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "AzSqlToLake",
						"type": "Copy",
						"dependsOn": [
							{
								"activity": "setCurrentDate",
								"dependencyConditions": [
									"Succeeded"
								]
							},
							{
								"activity": "Is_cdcOrBackDateLoad",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "AzureSqlSource",
								"sqlReaderQuery": {
									"value": "SELECT * \nFROM @{pipeline().parameters.db_schema}.@{pipeline().parameters.db_table} \nWHERE @{pipeline().parameters.db_col} > '@{formatDateTime(variables('fromDate'), 'yyyy-MM-dd HH:mm:ss')}'",
									"type": "Expression"
								},
								"queryTimeout": "02:00:00",
								"partitionOption": "None"
							},
							"sink": {
								"type": "ParquetSink",
								"storeSettings": {
									"type": "AzureBlobFSWriteSettings"
								},
								"formatSettings": {
									"type": "ParquetWriteSettings"
								}
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "ds_AzSqlDB",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "ds_parquet_dynamic",
								"type": "DatasetReference",
								"parameters": {
									"container": {
										"value": "@pipeline().parameters.container",
										"type": "Expression"
									},
									"layer": {
										"value": "@pipeline().parameters.layer",
										"type": "Expression"
									},
									"parqPath": {
										"value": "@concat(pipeline().parameters.source,'/',pipeline().parameters.db_table)",
										"type": "Expression"
									},
									"parqFile": {
										"value": "@concat(pipeline().parameters.db_table,'_',variables('currentDate'))",
										"type": "Expression"
									}
								}
							}
						]
					},
					{
						"name": "IsDataLoaded",
						"type": "IfCondition",
						"dependsOn": [
							{
								"activity": "AzSqlToLake",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@greater(activity('AzSqlToLake').output.rowsRead,0) ",
								"type": "Expression"
							},
							"ifFalseActivities": [
								{
									"name": "CopyToArchive",
									"type": "Copy",
									"dependsOn": [],
									"policy": {
										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"source": {
											"type": "ParquetSource",
											"storeSettings": {
												"type": "AzureBlobFSReadSettings",
												"recursive": true,
												"enablePartitionDiscovery": false
											},
											"formatSettings": {
												"type": "ParquetReadSettings"
											}
										},
										"sink": {
											"type": "ParquetSink",
											"storeSettings": {
												"type": "AzureBlobFSWriteSettings"
											},
											"formatSettings": {
												"type": "ParquetWriteSettings"
											}
										},
										"enableStaging": false,
										"translator": {
											"type": "TabularTranslator",
											"typeConversion": true,
											"typeConversionSettings": {
												"allowDataTruncation": true,
												"treatBooleanAsNumber": false
											}
										}
									},
									"inputs": [
										{
											"referenceName": "ds_parquet_dynamic",
											"type": "DatasetReference",
											"parameters": {
												"container": {
													"value": "@pipeline().parameters.container",
													"type": "Expression"
												},
												"layer": {
													"value": "@pipeline().parameters.layer",
													"type": "Expression"
												},
												"parqPath": {
													"value": "@concat(pipeline().parameters.source,'/',pipeline().parameters.db_table)",
													"type": "Expression"
												},
												"parqFile": {
													"value": "@concat(pipeline().parameters.db_table,'_',variables('currentDate'))",
													"type": "Expression"
												}
											}
										}
									],
									"outputs": [
										{
											"referenceName": "ds_parquet_dynamic",
											"type": "DatasetReference",
