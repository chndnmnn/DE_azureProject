							}
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"parameters": {
					"db_schema": {
						"type": "string",
						"defaultValue": "dbo"
					},
					"db_table": {
						"type": "string",
						"defaultValue": "DimUser"
					},
					"db_col": {
						"type": "string",
						"defaultValue": "updated_at"
					},
					"container": {
						"type": "string",
						"defaultValue": "azureproject"
					},
					"layer": {
						"type": "string",
						"defaultValue": "bronze"
					},
					"source": {
						"type": "string",
						"defaultValue": "azSqlDB"
					},
					"fromDate": {
						"type": "string"
					}
				},
				"variables": {
					"currentDate": {
						"type": "String"
					},
					"fromDate": {
						"type": "String"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/ds_AzSqlDB')]",
				"[concat(variables('factoryId'), '/datasets/ds_parquet_dynamic')]",
				"[concat(variables('factoryId'), '/linkedServices/ls_AzSqlDb')]",
				"[concat(variables('factoryId'), '/datasets/ds_json_dynamic')]"
			]
		},
		{
