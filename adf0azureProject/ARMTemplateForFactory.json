												"container": {
													"value": "@pipeline().parameters.container",
													"type": "Expression"
												},
												"layer": {
													"value": "@pipeline().parameters.layer",
													"type": "Expression"
												},
												"parqPath": "@concat(pipeline().parameters.source,'/',pipeline().parameters.db_table)",
												"parqFile": "@concat(pipeline().parameters.db_table,'_',variables('currentDate'))"
											}
										},
										"enableLogging": false,
										"storeSettings": {
											"type": "AzureBlobFSReadSettings",
											"recursive": true,
											"enablePartitionDiscovery": false
										}
									}
								}
							],
							"ifTrueActivities": [
								{
									"name": "GetLastLoadDate",
